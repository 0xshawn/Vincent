  "version": "0.0.1",
  "description": "JWT verification SDK for browser and backend",
  "author": "Lit Protocol",
  "license": "ISC",
  "type": "commonjs",
  "main": "dist/vincent-sdk.cjs.js",
  "module": "dist/vincent-sdk.esm.js",
  "browser": "dist/vincent-sdk.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
